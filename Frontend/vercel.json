{
    "rewrites": [
        {
         "source": "/api/:path*",
         "destination": "https://youtube-x-ecru.vercel.app/:path*"
  }
    ],
    "headers": [
        {
           "source": "/(.*)",
           "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://youtube-x-wf6x.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
        }
    ]
    
}
